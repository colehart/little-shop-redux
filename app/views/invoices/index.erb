<header Z>
  <a href="/invoices-dashboard">Dashboard</a>
  <h1>Invoices</h1>
</header>

<main>
  <section class="table" aria-label="invoice_list">
    <table>
      <tbody>
        <% @invoices.each do |invoice| %>
        <tr>
          <td><a href="/invoices/<%= invoice.id %>"><%= invoice.id %></a><a href="/invoices/<%= invoice.id  %>/edit"><button>Edit</button></a><form action="/invoices/<%= invoice.id %>" method="POST"><input type="hidden" name="_method" value="DELETE"><input type="submit" value="Delete"/>
          </form>
          </td>
        </tr>
        <% end %>
      </tbody>
    </table>
  </section>
</main>
